@model Setting
<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="mb-3 col-md-4">
                <label asp-for="Key" class="form-label">Key</label>
                <input class="form-control" asp-for="Key">
                <span asp-validation-for="Key" style="color:red"></span>
            </div>
            <div class="mb-3 col-md-4">
                <label asp-for="Value" class="form-label">Value</label>
                <input class="form-control" asp-for="Value">
                <span asp-validation-for="Value" style="color:red"></span>
            </div>
         </div>      
        <button type="submit" class="btn btn-primary">Submit</button>
        <a asp-action="Index" class="btn btn-light">Go Back</a>
    </form>

</div>
